    "================================================================================"
  ],
  "name": "@aurodesignsystem/aurokit",
  "version": "1.0.0-beta.4",
  "description": "Ingle install Auro ecosystem install",
  "repository": {
    "type": "git",
    "url": "https://github.com/AlaskaAirlines/aurokit"
  },
  "type": "module",
  "license": "Apache-2.0",
  "engines": {
    "node": "^20.x || ^22.x "
  },
